{
	"name": "simple-vui",
	"version": "0.0.1-beta.6",
	"private": false,
	"files": [
		"dist"
	],
	"main": "./dist/core.mjs",
	"types": "./dist/index.d.ts",
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc --noEmit && vite build && vue-tsc --declaration --emitDeclarationOnly",
		"preview": "vite preview",
		"test:unit": "vitest",
		"build-only": "vite build",
		"type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
		"lint": "eslint packages",
		"format": "prettier --write .",
		"prepare": "husky install",
		"release": "bumpp -r",
		"storybook": "storybook dev -p 6006",
		"storybook:build": "storybook build"
	},
	"dependencies": {
		"vue": "^3.3.4"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.6.3",
		"@commitlint/config-conventional": "^17.6.3",
		"@longo-andrea/eslint-config-base": "1.0.0-beta.3",
		"@longo-andrea/eslint-config-typescript": "1.0.0-beta.3",
		"@longo-andrea/eslint-config-vue": "1.0.0-beta.3",
		"@longo-andrea/prettier": "1.0.0-beta.3",
		"@rushstack/eslint-patch": "^1.2.0",
		"@storybook/addon-essentials": "^7.0.12",
		"@storybook/addon-interactions": "^7.0.12",
		"@storybook/addon-links": "^7.0.12",
		"@storybook/addon-styling": "^1.0.8",
		"@storybook/blocks": "^7.0.12",
		"@storybook/testing-library": "^0.0.14-next.2",
		"@storybook/vue3": "^7.0.12",
		"@storybook/vue3-vite": "^7.0.12",
		"@tsconfig/node18": "^2.0.1",
		"@types/jsdom": "^21.1.1",
		"@types/node": "^18.16.13",
		"@vitejs/plugin-vue": "^4.2.3",
		"@vitejs/plugin-vue-jsx": "^3.0.1",
		"@vue/eslint-config-prettier": "^7.1.0",
		"@vue/eslint-config-typescript": "^11.0.3",
		"@vue/test-utils": "^2.3.2",
		"@vue/tsconfig": "^0.4.0",
		"bumpp": "^9.1.0",
		"eslint": "^8.41.0",
		"eslint-plugin-storybook": "^0.6.12",
		"eslint-plugin-vue": "^9.13.0",
		"husky": "^8.0.3",
		"jsdom": "^22.0.0",
		"lint-staged": "^13.2.2",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.8.8",
		"pretty-quick": "^3.1.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"sass": "^1.62.1",
		"storybook": "^7.0.12",
		"typescript": "~5.0.4",
		"vite": "^4.3.8",
		"vitest": "^0.31.1",
		"vue-tsc": "^1.6.5"
	},
	"lint-staged": {
		"./src/*": "eslint --cache --fix"
	}
}
