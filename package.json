{
	"name": "simple-vui",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "vite",
		"build": "run-p type-check build-only",
		"preview": "vite preview",
		"test:unit": "vitest",
		"build-only": "vite build",
		"type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
		"lint": "eslint packages",
		"format": "prettier --write .",
		"prepare": "husky install",
		"release": "bumpp -r"
	},
	"dependencies": {
		"vue": "^3.3.4"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.6.3",
		"@commitlint/config-conventional": "^17.6.3",
		"@longo-andrea/eslint-config-base": "1.0.0-beta.2",
		"@longo-andrea/eslint-config-typescript": "1.0.0-beta.2",
		"@longo-andrea/eslint-config-vue": "1.0.0-beta.2",
		"@longo-andrea/prettier": "1.0.0-beta.2",
		"@rushstack/eslint-patch": "^1.2.0",
		"@tsconfig/node18": "^2.0.1",
		"@types/jsdom": "^21.1.1",
		"@types/node": "^18.16.13",
		"@vitejs/plugin-vue": "^4.2.3",
		"@vitejs/plugin-vue-jsx": "^3.0.1",
		"@vue/eslint-config-prettier": "^7.1.0",
		"@vue/eslint-config-typescript": "^11.0.3",
		"@vue/test-utils": "^2.3.2",
		"@vue/tsconfig": "^0.4.0",
		"bumpp": "^9.1.0",
		"eslint": "^8.41.0",
		"eslint-plugin-vue": "^9.13.0",
		"husky": "^8.0.3",
		"jsdom": "^22.0.0",
		"lint-staged": "^13.2.2",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.8.8",
		"pretty-quick": "^3.1.3",
		"typescript": "~5.0.4",
		"vite": "^4.3.8",
		"vitest": "^0.31.1",
		"vue-tsc": "^1.6.5"
	},
	"lint-staged": {
		"./src/*": "eslint --cache --fix"
	}
}
